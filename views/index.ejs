<% include partials/header %>
<div id="calendar-container">
    <div class="calendar-heading">
        <p id="year"></p>
        <button class="glyphicon glyphicon-arrow-left" id="prevBtn"></button>
        <span id="monthTitle" data-month="" data-year=""></span>
        <button class="glyphicon glyphicon-arrow-right" id="nextBtn"></button>
    </div>
    <table id="calendar">
        <tr>
            <th>S</th>
            <th>M</th>
            <th>T</th>
            <th>W</th>
            <th>Th</th>
            <th>F</th>
            <th>Sa</th>
        </tr>
    </table>
</div>
<% include partials/footer %>